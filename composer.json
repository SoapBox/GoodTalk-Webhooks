{
    "name": "soapbox/webhooks",
    "description": "Handle webhook requests from the API",
    "authors": [
        {
            "name": "Dave Wong",
            "email": "dave@soapboxhq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/http": "^7.0",
        "illuminate/validation": "^7.0",
        "soapbox/signed-requests": "^4.1",
        "nesbot/carbon": "^1.22 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.3",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "SoapBox\\Webhooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoapBox\\Webhooks\\WebhookServiceProvider"
            ]
        }
    }
}
